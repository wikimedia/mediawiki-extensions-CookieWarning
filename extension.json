{
	"name": "CookieWarning",
	"version": "0.3.0",
	"author": [
		"Florian Schmidt",
		"Liz Lee",
		"Jack Phoenix",
		"Ostrzyciel",
		"Octfx"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:CookieWarning",
	"descriptionmsg": "cookiewarning-desc",
	"type": "other",
	"license-name": "MIT",
	"requires": {
		"MediaWiki": ">= 1.35.0"
	},
	"MessagesDirs": {
		"CookieWarning": [
			"i18n"
		]
	},
	"Hooks": {
		"SkinAfterContent": "SkinHooks",
		"GetPreferences": "PreferenceHooks",
		"BeforeInitialize": "BeforeHooks",
		"BeforePageDisplay": "BeforeHooks",
		"PreferencesFormPreSave": "PreferenceHooks",
		"WebResponseSetCookieHook": "CookieHooks"
	},
	"HookHandlers": {
		"BeforeHooks": {
			"class": "MediaWiki\\Extension\\CookieWarning\\Hooks\\BeforeHooks"
		},
		"PreferenceHooks": {
			"class": "MediaWiki\\Extension\\CookieWarning\\Hooks\\PreferenceHooks"
		},
		"SkinHooks": {
			"class": "MediaWiki\\Extension\\CookieWarning\\Hooks\\SkinHooks"
		},
		"CookieHooks": {
			"class": "MediaWiki\\Extension\\CookieWarning\\Hooks\\CookieHooks"
		}
	},
	"config": {
		"CookieWarningEnabled": {
			"value": false,
			"description": "Enables the extension"
		},
		"CookieWarningUnsetCookies": {
			"value": false,
			"description": "Removes all cookies that aren't necessary until the user gave his consent"
		},
		"CookieWarningRequiredCookies": {
			"value": [
				"cookiewarning_dismissed",
				"cookiewarning_preferences",
				"UserID",
				"UserName",
				"_session"
			],
			"description": "List of cookie names that are deemed required"
		},
		"CookieWarningPreferenceCookies": {
			"value": [],
			"description": "List of cookie names that are categorized as preference"
		},
		"CookieWarningStatisticCookies": {
			"value": [],
			"description": "List of cookie names that are categorized as statistics"
		},
		"CookieWarningMarketingCookies": {
			"value": [],
			"description": "List of cookie names that are categorized as marketing"
		},
		"CookieWarningMoreUrl": {
			"value": "",
			"description": ""
		},
		"CookieWarningGeoIPServiceURL": {
			"value": "//freegeoip.net/json/",
			"description": ""
		},
		"CookieWarningGeoIPLookup": {
			"value": "none",
			"description": ""
		},
		"CookieWarningForCountryCodes": {
			"value": {
				"EU": "Europe",
